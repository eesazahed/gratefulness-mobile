{
  "expo": {
    "name": "gratefultime",
    "slug": "gratefultime",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/gratefultimelogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.hackclub.gratefultime",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gratefultimelogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gratefultimelogo.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-router",
      [
        "expo-notifications",
        {
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gratefultimelogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bcb71800-104e-49ff-bb97-26dbae6cace9"
      }
    },
    "owner": "eszhd"
  }
}
